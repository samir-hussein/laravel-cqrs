{
    "name": "samir-hussein/laravel-cqrs",
    "description": "A clean CQRS (Command Query Responsibility Segregation) pattern implementation for Laravel applications",
    "keywords": ["laravel", "cqrs", "command", "query", "pattern", "architecture"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samir Hussein",
            "email": "samirhussein274@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/contracts": ">=9.0",
        "illuminate/support": ">=9.0",
        "illuminate/container": ">=9.0",
        "illuminate/validation": ">=9.0",
        "illuminate/console": ">=9.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "orchestra/testbench": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelCQRS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCQRS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCQRS\\CQRSServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}

